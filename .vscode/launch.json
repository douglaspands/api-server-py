    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: FastAPI",
            "type": "python",
            "request": "launch",
            "module": "uvicorn",
            "args": [
                "--factory",
                "apiserver.main:create_app",
                "--reload"
            ],
            "jinja": false
        },
        {
            "name": "Python: Alembic",
            "type": "python",
            "request": "launch",
            "module": "alembic",
            "args": [
                "upgrade",
                "head",
            ],
        },
        {
            "name": "Python: Pytest",
            "type": "python",
            "request": "launch",
            "module": "pytest",
            "args": [
                "-vv"
            ],
        }
    ]
